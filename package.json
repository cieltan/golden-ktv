{
  "name": "golden-ktv",
  "version": "2.0.0",
  "description": "A web app for karaoke enthusiasts.",
  "engines": {
    "node": ">= 7.0.0"
  },
  "main": "index.js",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "deploy": "script/deploy",
    "heroku-token": "script/encrypt-heroku-auth-token",
    "lint": "eslint ./ --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "prepare": "if [ -d .git ]; then npm-merge-driver install; fi",
    "prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\"",
    "postinstall": "touch secrets.js",
    "seed": "node script/seed.js",
    "start": "node server",
    "start-dev": "NODE_ENV='development' npm run build-client-watch & NODE_ENV='development' npm run start-server",
    "start-server": "nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV='test' mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" \"./script/**/*.spec.js\" --require @babel/polyfill --require @babel/register"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "npm run lint-fix",
      "git add"
    ],
    "*.{css,scss,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "Fullstack Academy of Code",
  "license": "MIT",
  "dependencies": {
    "@opentok/client": "^2.16.3",
    "axios": "^0.19.0",
    "bulma": "^0.7.5",
    "compression": "^1.7.3",
    "connect-session-sequelize": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "history": "^4.10.1",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.1",
    "opentok": "^2.9.1",
    "opentok-react": "^0.9.0",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "pg": "^7.15.1",
    "pg-hstore": "^2.3.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-bulma-components": "^2.3.0",
    "react-dom": "^16.12.0",
    "react-player": "^1.14.2",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.21.3",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-proposal-decorators": "7.4.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/register": "^7.7.7",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0",
    "eslint-config-fullstack": "^6.0.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "npm-merge-driver": "^2.3.6",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-mock-store": "^1.5.4",
    "supertest": "^4.0.2",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  }
}
